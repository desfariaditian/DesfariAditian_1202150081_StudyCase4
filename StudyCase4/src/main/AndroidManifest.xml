<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.defaa.desfariaditian_1202150081_modul4.StudyCase4">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application>
        <activity android:name=".MainActivity">
            <intent-filter android:order="1">
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="example.com"
                    android:pathPattern="/.*"
                    android:scheme="https" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".DaftarMahasiwa"
            android:configChanges="orientation|screenSize"
            android:label="List Nama Mahasiswa"/>

        <activity android:name=".CariGambar"
            android:configChanges="orientation|screenSize"
            android:label="Pencari Gambar"></activity>
    </application>

</manifest>